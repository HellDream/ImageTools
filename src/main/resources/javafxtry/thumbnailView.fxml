   <children>
      <ImageView fx:id="thumbnailImageView" fitHeight="100.0" fitWidth="100.0" layoutX="25.0" layoutY="22.0" pickOnBounds="true" preserveRatio="true" />
      <Button layoutX="49.0" layoutY="135.0" mnemonicParsing="false" onAction="#saveThumbnail" prefHeight="26.0" prefWidth="53.0" text="Save">
         <font>
            <Font name="Arial" size="10.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
